\n            \n            return this.activities.filter(activity => activity.category === this.category)\n        }\n    },\n    components: {\n        Act,\n        AddForm\n    },\n    methods: {\n        moveLeft(id) {\n            let obj = {\n                id,\n                category: this.category\n            }\n            this.$emit('moveLeft', obj)\n        },\n        moveRight(id) {\n            let obj = {\n                id,\n                category: this.category\n            }\n            this.$emit('moveRight', obj)\n        },\n        showDetails() {\n            this.$emit('changeSubPage', 'detailsSubPage')\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"f09a3fa35688c3ba7be2302cf64b26fc","cacheData":{"env":{}}}